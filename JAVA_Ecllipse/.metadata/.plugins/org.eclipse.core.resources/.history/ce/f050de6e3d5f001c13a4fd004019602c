package encapsulation;

public class Asme extends Samsun{
	
	private String name;
	private String age;
	private String id;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public void run() {
		System.out.println("My name is "+name);
	}
	
}
